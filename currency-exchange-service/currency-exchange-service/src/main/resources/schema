create table exchange_value(id int primary key,currency_from varchar2(10),currency_to varchar2(10),conversion_multiple decimal,port varchar2(10));

insert into exchange_value(id,currency_from,currency_to,conversion_multiple,port)
values(10001,'USD','INR',65,null);
insert into exchange_value(id,currency_from,currency_to,conversion_multiple,port)
values(10002,'EUR','INR',75,null);
insert into exchange_value(id,currency_from,currency_to,conversion_multiple,port)
values(10003,'AUD','INR',25,null);

select * from exchange_value;

commit;